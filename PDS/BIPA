 MACRO
&NAME BIPA &A,&DA
 COPY SYSGBL
 SPLIT &DA
 AIF (&BIPA).NODC
&BIPA SETB 1
 CNOP 0,8
 B *+16
BIPAWORK DC D'0'
.NODC AIF ('&OT'  NE 'REG').NOREG
&NAME LR 1,&O
 AGO .COMMON
.NOREG AIF (&OL NE 1).NOBYTE
&NAME SR 1,1
 IC 1,&O
 AGO .COMMON
.NOBYTE AIF (&OL NE 2).NOHW
&NAME LH 1,&O
 AGO .COMMON
.NOHW ANOP
&NAME L 1,&O
.COMMON CVD 1,BIPAWORK
 AIF ('&A' EQ 'BIPAWORK(8)').MEXIT
 SPLIT &A
 MVC &A,BIPAWORK+8-&OL
.MEXIT MEND
